<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backgroundlight"
    tools:context="ui.resident.ResidentFragment">

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="389dp"
        android:layout_height="0dp"
        android:layout_marginBottom="58dp"
        android:src="@drawable/contact_person_info"
        app:layout_constraintBottom_toTopOf="@+id/txtContactPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="386dp"
        android:layout_height="274dp"
        android:layout_marginTop="4dp"
        android:src="@drawable/contact_box"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView6" />

    <TextView
        android:id="@+id/txtContactPersonName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="89dp"
        android:layout_marginLeft="89dp"
        android:layout_marginBottom="51dp"
        android:text="Navn Kontaktperson"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/txtContactPersonPhone"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView6" />

    <TextView
        android:id="@+id/txtContactPersonPhone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="41dp"
        android:text="Telefon Kontaktperson"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/txtEmailContactPerson"
        app:layout_constraintStart_toStartOf="@+id/txtContactPersonName"
        app:layout_constraintTop_toBottomOf="@+id/txtContactPersonName" />

    <TextView
        android:id="@+id/txtEmailContactPerson"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="268dp"
        android:text="Email Kontaktperson"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/txtContactPersonPhone"
        app:layout_constraintTop_toBottomOf="@+id/txtContactPersonPhone" />

    <ImageView
        android:id="@+id/btnMoreAboutresident"
        android:layout_width="162dp"
        android:layout_height="60dp"
        android:src="@drawable/btn_borger"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.859"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView7"
        app:layout_constraintVertical_bias="0.404" />
</androidx.constraintlayout.widget.ConstraintLayout>