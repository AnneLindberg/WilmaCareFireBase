<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.wilmacarefirebase.ui.home.HomeFragment"
        android:label="Profil"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.wilmacarefirebase.ui.dashboard.DashboardFragment"
        android:label="Opslagstavlen"
        tools:layout="@layout/fragment_dashboard">
    </fragment>

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.wilmacarefirebase.ui.calender.CalenderFragment"
        android:label="Kalender"
        tools:layout="@layout/fragment_calender" />

    <fragment
        android:id="@+id/navigation_residentProfile"
        android:name="com.example.wilmacarefirebase.ui.resident.ResidentFragment"
        android:label="fragment_resident"
        tools:layout="@layout/fragment_resident" />
    <fragment
        android:id="@+id/navigation_addPost"
        android:name="com.example.wilmacarefirebase.ui.dashboard.AddPostFragment"
        android:label="Lav Opslag"
        tools:layout="@layout/fragment_add_post" />
</navigation>